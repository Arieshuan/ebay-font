    "istanbul": "^0.4.0",
    "jsdom": "^9.0.0",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-lasso": "^3.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "lasso": "^2.0.0",
    "lasso-less": "^2.4.5",
    "lasso-marko": "^2.3.0",
    "less": "^2.7.1",
